{
    "name": "ipeweb/ipesheets",
    "autoload": {
        "psr-4": {
            "Ipeweb\\RecapSheets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RecapSheets\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpunit": "vendor/bin/phpunit tests --colors",
        "phpcs": "vendor/bin/phpcs ./src -n --colors"
    },
    "authors": [
        {
            "name": "Murilo"
        }
    ],
    "require": {
        "php": "^8.1",
        "phpmailer/phpmailer": "^6.9",
        "firebase/php-jwt": "^6.10"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5",
        "squizlabs/php_codesniffer": "^3.9"
    }
}